string Solution::longestCommonPrefix(vector<string> &v) {
    string ans;
    for(int i=0;i<v[0].size();i++)
    {
        int j=1;
        for(;j<v.size();j++)
        {
            if( i>=v[j].size() ||v[j][i]!=v[j-1][i])
            {
                break;
            }
        }
        if(j==v.size())
        {
            ans.push_back(v[0][i]);
        }
    }
    return ans;
    
}
